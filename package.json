{
	"name": "db-12-vscode",
	"displayName": "db-12-vscode",
	"publisher": "bernhardwalter",
	"description": "Commands to send code to Databricks cluster",
	"version": "0.2.0",
	"repository": "https://github.com/bernhard-42/db-12-vscode",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:db-12-vscode.initialize"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
				"command": "db-12-vscode.initialize",
				"title": "Databricks (API 1.2): Initialize Databricks Connections"
			},
			{
				"command": "db-12-vscode.sendSelectionOrLine",
				"title": "Databricks (API 1.2): Send selection or line"
			},
			{
				"command": "db-12-vscode.cancel",
				"title": "Databricks (API 1.2): Cancel the currently running command"
			},
			{
				"command": "db-12-vscode.stop",
				"title": "Databricks (API 1.2): Stop the current exection context"
			}
		],
		"keybindings": [{
			"command": "db-12-vscode.sendSelectionOrLine",
			"key": "ctrl+k shift+enter",
			"mac": "cmd+k shift+enter"
		}],
		"configuration": {
			"title": "DB-12",
			"properties": {
				"db-12-vscode.zip-command": {
					"type": "string",
					"default": "zip -r -FS --exclude=*.DS_Store* --exclude=*__pycache__*",
					"description": "Shell command to recursively create a zip file"
				},
				"db-12-vscode.databricks-cli": {
					"type": "string",
					"default": "/opt/miniconda/bin/databricks",
					"description": "Fully qualified path to databricks cli"
				},
				"db-12-vscode.profile": {
					"type": "string",
					"default": "",
					"description": "Databricks CLI profile"
				},
				"db-12-vscode.cluster": {
					"type": "string",
					"default": "",
					"description": "Databricks cluster"
				},
				"db-12-vscode.language": {
					"type": "string",
					"default": "",
					"description": "Databricks programming language (python, SQL, scala, r)"
				},
				"db-12-vscode.lib-folder": {
					"type": "string",
					"default": "",
					"description": "Local library folder"
				},
				"db-12-vscode.remote-folder": {
					"type": "string",
					"default": "",
					"description": "Remote dbfs folder"
				}
			}
		},
		"viewsContainers": {
			"activitybar": [{
				"id": "databricksExplorer",
				"title": "Databricks",
				"icon": "media/symbol.svg"
			}]
		},
		"views": {
			"databricksExplorer": [{
				"id": "databricksVariableExplorer",
				"name": "Variables"
			}]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.2",
		"@types/vscode": "^1.44.0",
		"@types/ini": "^1.3.30",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"vsce": "^1.75.0"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"ini": "^1.3.5"
	}
}