{
	"name": "databricks-run",
	"displayName": "databricks-run",
	"publisher": "bernhardwalter",
	"description": "Commands to send code to Databricks cluster",
	"version": "0.3.0",
	"repository": "https://github.com/bernhard-42/databricks-run",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:databricks-run.initialize"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
				"command": "databricks-run.initialize",
				"title": "Databricks Run: Initialize Databricks Connections"
			},
			{
				"command": "databricks-run.sendSelectionOrLine",
				"title": "Databricks Run: Send selection or line"
			},
			{
				"command": "databricks-run.cancel",
				"title": "Databricks Run: Cancel the currently running command"
			},
			{
				"command": "databricks-run.stop",
				"title": "Databricks Run: Stop the current exection context"
			}
		],
		"keybindings": [{
				"command": "databricks-run.sendSelectionOrLine",
				"key": "ctrl+k shift+enter",
				"mac": "cmd+k shift+enter",
				"when": "editorTextFocus"
			},
			{
				"command": "databricks-run.initialize",
				"key": "ctrl+k shift+i",
				"mac": "cmd+k shift+i",
				"when": "editorTextFocus"
			},
			{
				"command": "databricks-run.stop",
				"key": "ctrl+k shift+s",
				"mac": "cmd+k shift+s",
				"when": "editorTextFocus"
			},
			{
				"command": "databricks-run.cancel",
				"key": "ctrl+k shift+c",
				"mac": "cmd+k shift+c",
				"when": "editorTextFocus"
			}
		],
		"configuration": {
			"title": "DB-12",
			"properties": {
				"databricks-run.zip-command": {
					"type": "string",
					"default": "zip -r -FS --exclude=*.DS_Store* --exclude=*__pycache__*",
					"description": "Shell command to recursively create a zip file"
				},
				"databricks-run.databricks-cli": {
					"type": "string",
					"default": "/opt/miniconda/bin/databricks",
					"description": "Fully qualified path to databricks cli"
				},
				"databricks-run.profile": {
					"type": "string",
					"default": "",
					"description": "Databricks CLI profile"
				},
				"databricks-run.cluster": {
					"type": "string",
					"default": "",
					"description": "Databricks cluster"
				},
				"databricks-run.python": {
					"type": "object",
					"properties": {
						"lib-folder": {
							"type": "string",
							"default": "",
							"description": "Local library folder"
						},
						"remote-folder": {
							"type": "string",
							"default": "",
							"description": "Remote dbfs folder"
						}
					}
				}
			}
		},
		"viewsContainers": {
			"activitybar": [{
				"id": "databricksExplorer",
				"title": "Databricks",
				"icon": "media/databricks-run.png"
			}]
		},
		"views": {
			"databricksExplorer": [{
					"id": "databricksVariableExplorer",
					"name": "Python Variables"
				},
				{
					"id": "databricksLibraryExplorer",
					"name": "Python Libraries"
				}
			]
		},
		"menus": {
			"view/title": []
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.2",
		"@types/vscode": "^1.44.0",
		"@types/ini": "^1.3.30",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"vsce": "^1.75.0"
	},
	"dependencies": {
		"@types/decomment": "^0.9.0",
		"axios": "^0.19.2",
		"decomment": "^0.9.3",
		"ini": "^1.3.5"
	}
}